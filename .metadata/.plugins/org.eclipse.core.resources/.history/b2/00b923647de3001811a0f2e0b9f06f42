package org.mathi.thread;

public class ThreadDemoJoin {


	public static void main(String[] args) {
		ThreadDemo41 td=new ThreadDemo41();
		td.start();
	}

}


class ThreadDemo41 implements Runnable {

	@Override
	public void run() {
		System.out.println("Execution Started - "+Thread.currentThread().getName());
		
	}

}
